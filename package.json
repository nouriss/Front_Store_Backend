{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "dev-up": "db-migrate --env dev up -m ./migrations   --config ./database.json",
    "test-up": "db-migrate --env test up -m ./migrations --config ./database.json",
    "test-down": "db-migrate reset --env test -m ./migrations  --config ./database.json",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "dry-test": "npm run test-up && cross-env ENV=test npm run watch",
    "test": "npm run test-up && cross-env ENV=test jasmine-ts && npm run test-down",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@ert78gb/jasmine-ts": "^0.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.7",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.9.4"
  }
}
